{
  "name": "semantic-release-workspace-dependency",
  "version": "0.0.0-dev",
  "license": "MIT",
  "description": "semantic-release plugin to update version of workspace dependency",
  "keywords": [
    "workspace",
    "workspaces",
    "semantic-release",
    "semantic-release-plugin"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "dependencies": {
    "@antongolub/git-root": "^1.5.7",
    "@npmcli/map-workspaces": "^2.0.4",
    "@semantic-release/error": ">=3 <=4",
    "aggregate-error": "3.X",
    "ensure-error": "3.X",
    "find-versions": "4.X",
    "read-package-json-fast": "^2.0.3",
    "semver": "^7.3.7"
  },
  "scripts": {
    "test": "npm run eslint && npm run tscheck && npm run test:cov",
    "test:r": "vitest run src",
    "test:cov": "vitest run src --coverage",
    "test:w": "vitest watch src",
    "lint": "npm run eslint && npm run tscheck",
    "eslint": "node node_modules/eslint/bin/eslint \"src/**/*.{ts,tsx}\"",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "eslint:fix": "npm run eslint -- --fix",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "format:src": "prettier src --write",
    "commit": "cz",
    "build": "sh Taskfile build_pkgroll",
    "lint:w": "run-p tscheck:w eslint:w"
  },
  "peerDependencies": {
    "semantic-release": ">=18"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/lodash": "^4.14.182",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^18.6.4",
    "@types/semantic-release": "^17.2.4",
    "@types/semver": "^7.3.10",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "c8": "^7.12.0",
    "commitizen": "^4.2.5",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-etc": "^2.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-regexp": "^1.8.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "eslint-plugin-wix-editor": "^3.3.0",
    "lodash": "^4.17.21",
    "mock-fs": "^5.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "request": "^2.88.2",
    "semantic-release": "^19.0.3",
    "tslib": "^2.4.0",
    "type-fest": "^2.18.0",
    "typescript": "^4.7.4",
    "vitest": "^0.21.0",
    "watchexec-bin": "^1.0.0"
  }
}
